<header id="topnav">
  <div class="topbar-main">
    <div class="container">
      <!-- Logo container-->
      <%= yield :logo %>
      <!-- End Logo container-->
      <div class="menu-extras">

        <ul class="nav navbar-nav navbar-right pull-right">
          <%= yield :search %>
          <%= yield :profile %>
        </ul>

        <div class="menu-item">
          <!-- Mobile menu toggle-->
          <a class="navbar-toggle">
            <div class="lines">
              <span></span>
              <span></span>
              <span></span>
            </div>
          </a>
          <!-- End mobile menu toggle-->
        </div>
      </div>

    </div>
  </div>

  <div class="navbar-custom">
    <div class="container">
      <div id="navigation">
        <!-- Navigation Menu-->
        <ul class="navigation-menu">
          <%= yield :nav %>
        </ul>
        <!-- End navigation menu        -->
      </div>
    </div>
  </div>
</header>
        